<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.IntroController">
   <children>
      <TextField alignment="BOTTOM_CENTER" editable="false" prefHeight="25.0" prefWidth="181.0" text="Maze Generation Programme" />
      <TextField fx:id="usernameTextField" promptText="Username" GridPane.rowIndex="1" />
      <Button fx:id="enterButton" alignment="BOTTOM_CENTER" mnemonicParsing="false" onAction="#onClickEnterButton" text="Enter" GridPane.halignment="CENTER" GridPane.rowIndex="2" GridPane.valignment="BASELINE" />
   </children>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
</GridPane>
